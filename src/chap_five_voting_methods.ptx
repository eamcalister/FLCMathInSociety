							<cell><delete>E: 1</delete></cell>
						</row>
						<row>
							<cell></cell>
							<cell><m>\underline{+2}</m></cell>
							<cell></cell>
							<cell><m>\underline{+1}</m></cell>
							<cell></cell>
						</row>
						<row>
