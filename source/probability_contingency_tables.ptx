						<cell>Computer</cell>
						<cell>10%</cell>
						<cell>5%</cell>
						<cell>15%</cell>
					</row>
					<row>
						<cell>No Computer</cell>
						<cell>15%</cell>
						<cell>70%</cell>
						<cell>85%</cell>
					</row>
					<row>
						<cell>Total</cell>
						<cell>25%</cell>
						<cell>75%</cell>
